<templateSet group="plog">
  <template name="plog" value="# $TIME$ $SUBJECT$&#10;{:tags [$TAGS$]}" description="Personal Log new entry" toReformat="false" toShortenFQNames="true">
    <variable name="TIME" expression="date(&quot;yyyy-MM-dd HH:mm&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="SUBJECT" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="TAGS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CSS" value="false" />
      <option name="GROOVY" value="false" />
      <option name="HTML" value="false" />
      <option name="JAVA_CODE" value="false" />
      <option name="JSON" value="false" />
      <option name="MAVEN" value="false" />
      <option name="OTHER" value="true" />
      <option name="SQL" value="false" />
      <option name="XML" value="false" />
    </context>
  </template>
  <template name="link" value="{:url $URL$&#10; :title $TITLE$&#10; :description $DESCRIPTION$&#10; :tags [$TAGS$]}" description="new Plog Link" toReformat="false" toShortenFQNames="true">
    <variable name="URL" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="TITLE" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="DESCRIPTION" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="TAGS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="plink" value="# $TIME$ $TITLE$&#10;{:tags [$TAGS$]}&#10;&#10;{:url $URL$&#10; :title $TITLE$&#10; :description $DESCRIPTION$&#10; :tags [$TAGS$]}" description="personal log with link" toReformat="false" toShortenFQNames="true">
    <variable name="TIME" expression="date(&quot;yyyy-MM-dd HH:mm&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="TITLE" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="TAGS" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="URL" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="DESCRIPTION" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="todo" value="[ ] " description="New todo" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
</templateSet>